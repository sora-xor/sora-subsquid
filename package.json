{
	"name": "kusama-transfers",
	"private": true,
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "rm -rf lib && tsc",
		"update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
		"db:migrate": "npx squid-typeorm-migration apply",
		"processor:start": "node lib/processor.js",
		"query-node:start": "squid-graphql-server --subscriptions --max-response-size 10000 --dumb-cache in-memory --dumb-cache-ttl 1000 --dumb-cache-size 100 --dumb-cache-max-age 1000",
		"generate-types": "ts-node ./scripts/prepare-typegen-json.ts && sqd typegen && ts-node ./scripts/generate-additional-types.ts",
		"deploy": "sqd deploy ./",
		"process": "sqd down && sqd up && sqd process"
	},
	"dependencies": {
		"@polkadot/api": "^10.0.1",
		"@polkadot/util": "^11.0.1",
		"@subsquid/archive-registry": "3.2.0",
		"@subsquid/graphql-server": "4.3.0",
		"@subsquid/ss58": "^0.1.4",
		"@subsquid/substrate-processor": "2.4.1",
		"@subsquid/typeorm-migration": "1.2.1",
		"@subsquid/typeorm-store": "1.2.2",
		"bignumber.js": "^9.1.1",
		"dotenv": "^16.0.0",
		"pg": "8.9.0",
		"type-fest": "^3.7.2",
		"type-graphql": "^1.2.0-rc.1",
		"typeorm": "0.3.11"
	},
	"devDependencies": {
		"@subsquid/substrate-metadata-explorer": "1.1.2",
		"@subsquid/substrate-typegen": "2.2.1",
		"@subsquid/typeorm-codegen": "1.3.1",
		"@types/node": "18.11.18",
		"ts-node": "^10.9.1",
		"typescript": "4.9.5",
		"yamljs": "^0.3.0"
	},
	"overrides": {
		"es5-ext": "0.10.35"
	}
}
